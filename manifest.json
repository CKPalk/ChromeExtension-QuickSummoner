{
	"manifest_version": 2,
	
	"name": "Quick Summoner",
	"description": "Grab quick information on any summoner, any time!",
	"version": "1.0",
	
	"browser_action": {
		"default_icon": "extension_button.png",
		"default_popup": "QuickSummonerHTML.html"
	},

	"permissions": [
		"storage"
	],
	
	"icons": {
		"48": "extension_icon_48.png"
	}
}